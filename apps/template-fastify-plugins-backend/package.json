  "scripts": {
    "build": "npm-run-all --sequential build:fastify",
    "build:astro": "astro build",
    "build:devenvironment": "npm-run-all --sequential compose db:migrate",
    "build:fastify": "tsc",
    "build:prod": "npm-run-all --sequential clear db:generate build",
    "build:serve": "npm-run-all --sequential clear db:generate build start",
    "build:svelte": "vite build",
    "build:test": "tsc --project ./tsconfig.test.json",
    "clear": "rm -rf dist && rm -f *.tsbuildinfo && rm -f .eslintcache && rm -rf build",
    "compose": "docker-compose up --no-recreate -d",
    "db:deploy": "prisma deploy",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:reset": "prisma reset",
    "deploy": "build docker image and push to registry",
    "dev": "npm-run-all --sequential clear db:generate build build:devenvironment --parallel dev-fastify",
    "dev-astro": "astro dev --port 4000",
    "dev-fastify": "tsc -w & nodemon --inspect dist/main.js",
    "dev-svelte": "vite dev",
    "dockerdev": "tsc && (tsc -w & nodemon dist/src/main.js)",
    "lint-all": "npm-run-all --sequential lint:tsc lint:eslint",
    "lint:eslint": "eslint --cache --ext .ts,.tsx src",
    "lint:tsc": "tsc --noEmit",
    "start": "node dist/src/main.js",
    "start:devenvironment": "docker-compose up --no-recreate -d && docker-compose logs -f templatefastify",
    "test": "npm-run-all --sequential clear build:test lint-all test:vitest",
    "test:vitest": "vitest run --coverage"
  },
  "type": "module"
