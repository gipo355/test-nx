    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "compile-ajv": "tsx ./scripts/compileAjv.ts",
    "compile-fast-json": "tsx ./scripts/compileFastJsonStringify.ts",
    "dev": "svelte-kit sync && infisical run --path=\"/spotify\" -- npm-run-all --sequential compile dev:svelte",
    "dev:storybook": "storybook dev -p 6006",
    "dev:svelte": "vite dev",
    "format": "prettier --plugin-search-dir . --write .",
    "lint": "prettier --plugin-search-dir . --check . --write && eslint . --fix",
    "postbuild": "npx svelte-sitemap --domain https://myawesomedomain.com",
