{
  "name": "testsveltekit",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/testsveltekit/src",
  "targets": {
    "check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "svelte-check",
        "cwd": "apps/testsveltekit"
      }
    },
    "add": {
      "executor": "@nxext/sveltekit:add"
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/testsveltekit/**/*.{ts,svelte,spec.ts}"
        ]
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/testsveltekit"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "testsveltekit:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "testsveltekit:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "testsveltekit:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "testsveltekit:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "testsveltekit:build:development"
        },
        "production": {
          "buildTarget": "testsveltekit:build:production"
        }
      }
    }
  },
  "tags": []
}
