{
  "name": "sveltekit",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/sveltekit/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/libs/sveltekit",
        "main": "libs/sveltekit/src/index.ts",
        "tsConfig": "libs/sveltekit/tsconfig.lib.json",
        "assets": [
          "libs/sveltekit/*.md",
          {
            "input": "./libs/sveltekit/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./libs/sveltekit/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./libs/sveltekit",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./libs/sveltekit",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/sveltekit/**/*.ts",
          "libs/sveltekit/package.json",
          "libs/sveltekit/generators.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/sveltekit/jest.config.ts"
      }
    }
  },
  "tags": []
}
